<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CustomerViewController">
   <children>
      <TableView fx:id="customerTV" layoutX="77.0" layoutY="76.0" prefHeight="200.0" prefWidth="325.0">
        <columns>
          <TableColumn fx:id="customerNameTC" prefWidth="136.0" text="Customer Name" />
          <TableColumn fx:id="customerPhoneTC" prefWidth="85.0" text="Phone" />
            <TableColumn fx:id="customerIdTC" prefWidth="60.0" text="ID" />
        </columns>
      </TableView>
      <Button fx:id="addButton" layoutX="83.0" layoutY="285.0" mnemonicParsing="false" text="Add" />
      <Button fx:id="editButton" layoutX="121.0" layoutY="285.0" mnemonicParsing="false" text="Edit" />
      <Button fx:id="deleteButton" layoutX="158.0" layoutY="285.0" mnemonicParsing="false" text="Delete" />
      <Button onAction="#Back" fx:id="backButton" layoutX="209.0" layoutY="285.0" mnemonicParsing="false" text="Back" />
   </children>
</AnchorPane>
